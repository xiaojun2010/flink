<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
    <property>
        <name>fs.defaultFS</name>
        <value>hdfs://hadoop:8020</value>
    </property>
    <property>
        <!-- 指明是否开启用户认证 -->
        <name>hadoop.security.authorization</name>
        <value>false</value>
    </property>
    <property>
	    <name>hadoop.tmp.dir</name>
	    <value>/dfs/tmp</value> 
    </property>
</configuration>
