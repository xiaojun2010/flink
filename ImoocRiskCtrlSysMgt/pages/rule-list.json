        "data": {
          "rows" : [
            {
              "id": 1,
              "type": "统计规则",
              "name": "同一地区的IP在最近1小时内注册账号数超过50次",
              "expression": "ireCount(register)>50"
            },
            {
              "id": 2,
              "type": "统计规则",
              "name": "账号在最近3分钟内登录次数超过5次",
              "expression": "ireCount(login)>5"
            },
            {
              "id": 3,
              "type": "行为规则",
              "name": "用户在最近3天内修改过手机号码且其他行为事件数量少于3",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 4,
              "type": "统计规则",
              "name": "最近10天无登录且促销活动有登录",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 5,
              "type": "静态规则",
              "name": "用户没有使用常用IP下单",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 6,
              "type": "统计规则",
              "name": "账号券领取的数量超过50张",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 7,
              "type": "统计规则",
              "name": "券领取的用户在最近1个月的IP更换次数超过5次",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 8,
              "type": "行为规则",
              "name": "使用券下单的用户的行为路径是'登录-领券-下单'",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 9,
              "type": "统计规则",
              "name": "购买商品类目数量小于3或者80%商品价格低于20元且90%商品复购次数低于2次",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 10,
              "type": "统计规则",
              "name": "商品交易时间间隔低于15分钟",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 11,
              "type": "统计规则",
              "name": "优惠券领取账户是注册时间少于30分钟",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            },
            {
              "id": 12,
              "type": "行为规则",
              "name": "账号等级为L2以上的用户各行为之间时间间隔平均少于3分钟或者浏览行为停留平均时间少于3分钟",
              "expression": "ireCount(profile)>1&&ireCount(seq)<3"
            }
           
          ]
        },
